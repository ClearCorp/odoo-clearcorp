<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>
<record model="ir.ui.view" id="project_issue_contact_form_view">
    <field name="name">Project Issue Contact</field>
    <field name="model">project.issue</field>
    <field name="inherit_id" ref="project_issue.project_issue_form_view" />
    <field name="arch" type="xml">
        <xpath expr="//form/sheet/group/group/field[@name='partner_id']" position="replace">
            <field name="partner_id" on_change="onchange_partner_id(partner_id)" 
                attrs="{'readonly':[('project_id','!=',False)]}"/>
        </xpath>
    </field>
</record>
</data>
</openerp>